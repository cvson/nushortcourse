%!GLoBES

$version="3.0.0"

/* ####### Beam flux ####### */

nuflux(#JHFplus)<
	@flux_file="JHFplus.dat"
	@time = 15	/* years */
	@power = 0.77	/* MW (proton intensity) */
	@norm = 10.918	/*10*pow(295,2)*6.022*1e32 *1e-38/(3*0.16)*/
>

nuflux(#JHFminus)<
	@flux_file="JHFminus.dat"
	@time = 15	/* years */
	@power = 0.77 	/* MW (proton intensity) */
	@norm = 10.918	/*10*pow(295,2)*6.022*1e32 *1e-38/(3*0.16)*/
>

/* ####### Detector settings ###### */

$target_mass = 	25.	/* kt (fiducial mass)*/

/* ####### Energy window ####### */

$bins = 	200
$emin = 	0.0	/* GeV */
$emax = 	5.0	/* GeV */


/* ####### Baseline setting ####### */

$profiletype =	 	1 
$baseline =		295.0	/* km */


/* ####### Technical information ####### */

$filter_state = 0 
$filter_value = 1000000 

/* ####### Energy resolution ####### */

energy(#ERES)<
	@type = 1
	@sigma_e = {0.0,0.03,0.085}
>

